<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="survey.xsl"?> 

<survey>

	<rate-scale>
		<rate value="1">Bad</rate>
		<rate value="2"></rate>
		<rate value="3"></rate>
		<rate value="4"></rate>
		<rate value="5">Good</rate>
	</rate-scale>

	<course>
		<field attr="course-name" default="ATNO-">Course Designator</field>
		<rating attr="course-content">Content</rating>
		<rating attr="course-labs">Labs</rating>
		<rating attr="course-org">Organization</rating>
		<question attr="course-comment">Comments</question>
	</course>
	
	<instructor>
		<field attr="inst-name-NUM">Instructor Name</field>
		<rating attr="inst-know-">Demonstrated Knowledge</rating>
		<rating attr="inst-prep-">Preparedness</rating>
		<rating attr="inst-comm-">Communication Skills</rating>
		<question attr="inst-comment-">Comments</question>
	</instructor>
	
</survey>
